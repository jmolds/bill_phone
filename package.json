{
  "name": "bill-phone",
  "version": "1.0.0",
  "description": "A kiosk phone app for Bill",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject"
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.3",
    "@react-native-community/netinfo": "11.4.1",
    "expo": "^53.0.9",
    "expo-keep-awake": "~14.1.4",
    "expo-status-bar": "~2.2.3",
    "metro": "^0.82.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-web": "^0.20.0",
    "react-native-webrtc": "^111.0.6",
    "socket.io-client": "^4.8.1",
    "uuid": "^9.0.1",
    "expo-dev-client": "~5.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true,
  "expo": {
    "name": "Bill's Phone",
    "slug": "bill-phone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2C3E50"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2C3E50"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
